pipeline
{
    agent any
    stages{
        stage ('build')
        {
            steps{
                echo 'Building'
            
                }
        }
        stage ('Test')
        {
            steps{
                echo 'Testing'
                
            }
        }
        stage ('Deploy'){
            steps{
                echo 'Deploying..'
            }
        }
        
    }
}
